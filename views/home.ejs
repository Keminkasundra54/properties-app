      formdata.append('room_information', mydata)
      formdata.append('_id', data._id)

      const urlEncoded = new URLSearchParams(formdata).toString()
      $.ajax({
        url: idToUpdate ? "/api/property/update" : "/api/property/create",
        method: 'post',
