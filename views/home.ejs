    $(document).on('submit', 'form.propertyForm ', function (e) {
      e.preventDefault();
      var data = getFormData($('form.propertyForm'));
      data['userId'] = 1;
     
      let idToUpdate = data._id;
      if (idToUpdate) {
      } else {
        delete data._id;
      }
      const mydata = JSON.stringify(roomes)
      for (let j = 0; j < myfile.length; j++) {
